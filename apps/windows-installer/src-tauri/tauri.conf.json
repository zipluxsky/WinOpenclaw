{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "OpenClaw Setup",
  "version": "0.1.0",
  "identifier": "ai.openclaw.setup",
  "build": {
    "frontendDist": "../ui",
    "devUrl": "http://localhost:1430",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "removeUnusedCommands": true
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "OpenClaw Setup",
        "width": 720,
        "height": 640,
        "minWidth": 560,
        "minHeight": 420,
        "resizable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico"],
    "windows": {
      "webviewInstallMode": {
        "type": "downloadBootstrapper",
        "silent": true
      }
    },
    "createUpdaterArtifacts": false
  },
  "plugins": {}
}
